// from the command palette.
[
  {
    "context": "(Editor && edit_prediction_conflict)",
    "bindings": {
      "cmd-enter": "editor::AcceptEditPrediction"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "ctrl-alt-l": "editor::Format"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "ctrl-alt-o": "editor::OrganizeImports"
    }
  }
]
